{
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "deno: debug",
            "type": "pwa-node",
            "program": "${workspaceFolder}/src/main.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect-brk",
                "--allow-all",
            ],
            "outputCapture": "std",
            "attachSimplePort": 9229
        }
    ]
}